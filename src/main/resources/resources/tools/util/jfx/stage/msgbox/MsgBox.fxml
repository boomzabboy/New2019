<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="1.7976931348623157E308" maxWidth="480.0" minWidth="480.0" prefWidth="480.0" spacing="3.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.l2jserver.tools.util.jfx.stage.msgbox.MsgBoxController">
   <children>
      <ImageView fx:id="ivIcon" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER">
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <VBox fx:id="vbContentArea" spacing="3.0" HBox.hgrow="ALWAYS">
         <children>
            <Label fx:id="lbHeadline" focusTraversable="false" maxHeight="32.0" maxWidth="1.7976931348623157E308" minHeight="32.0" prefHeight="32.0" VBox.vgrow="NEVER" />
            <Label fx:id="lbDescription" focusTraversable="false" maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
               <VBox.margin>
                  <Insets left="6.0" />
               </VBox.margin></Label>
            <HBox fx:id="hbButtons" alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" spacing="3.0" VBox.vgrow="NEVER" />
            <TextArea fx:id="taDetails" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0" VBox.vgrow="NEVER" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
</HBox>
